      <bootclasspath refid="aimerger_for_appinv_classic.module.bootclasspath"/>
      <classpath refid="aimerger_for_appinv_classic.module.production.classpath"/>
      <src refid="aimerger_for_appinv_classic.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.aimerger_for_appinv_classic"/>
    </javac2>
    
    <copy todir="${aimerger_for_appinv_classic.output.dir}">
      <fileset dir="${module.aimerger_for_appinv_classic.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.aimerger_for_appinv_classic.tests" depends="register.custom.compilers,compile.module.aimerger_for_appinv_classic.production" description="compile module aimerger_for_appinv_classic; test classes" unless="skip.tests"/>
  
  <target name="clean.module.aimerger_for_appinv_classic" description="cleanup module">
    <delete dir="${aimerger_for_appinv_classic.output.dir}"/>
    <delete dir="${aimerger_for_appinv_classic.testoutput.dir}"/>
  </target>
</project>
  </patternset>
  
  <path id="appengine.module.sourcepath">
    <dirset dir="${module.appengine.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  <path id="appengine.module.test.sourcepath">
    <dirset dir="${module.appengine.basedir}">
      <include name="tests"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.appengine" depends="compile.module.appengine.production,compile.module.appengine.tests" description="Compile module appengine"/>
  
  <target name="compile.module.appengine.production" depends="register.custom.compilers,compile.module.common,compile.module.build,compile.module.components" description="Compile module appengine; production classes">
    <mkdir dir="${appengine.output.dir}"/>
    <javac2 destdir="${appengine.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.appengine}/javac">
      <compilerarg line="${compiler.args.appengine}"/>
      <bootclasspath refid="appengine.module.bootclasspath"/>
      <classpath refid="appengine.module.production.classpath"/>
      <src refid="appengine.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.appengine"/>
    </javac2>
    
    <copy todir="${appengine.output.dir}">
      <fileset dir="${module.appengine.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.appengine.tests" depends="register.custom.compilers,compile.module.appengine.production" description="compile module appengine; test classes" unless="skip.tests">
    <mkdir dir="${appengine.testoutput.dir}"/>
    <javac2 destdir="${appengine.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.appengine}/javac">
      <compilerarg line="${compiler.args.appengine}"/>
      <bootclasspath refid="appengine.module.bootclasspath"/>
      <classpath refid="appengine.module.classpath"/>
      <src refid="appengine.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.appengine"/>
    </javac2>
    
    <copy todir="${appengine.testoutput.dir}">
      <fileset dir="${module.appengine.basedir}/tests">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.appengine" description="cleanup module">
    <delete dir="${appengine.output.dir}"/>
    <delete dir="${appengine.testoutput.dir}"/>
  </target>

        <ai:Icon ui:field='projectNameSortAsc' icon="arrow_upward" visible="false"
                 styleName="gwt-InlineLabel"/>
        </g:FlowPanel>
        <g:FlowPanel styleName="gwt-Label ode-ProjectHeaderLabel">
